{
    "name": "setlx",
    "displayName": "SetlXHelper",
    "icon": "setlx_logo.png",
    "version": "1.3.1",
    "publisher": "LucaVazz",
    "description": "A plugin for VS Code to make writing SetlX-programs comfortable. ",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/LucaVazz/SetlXHelper"
    },
    "bugs": {
        "url": "https://github.com/LucaVazz/SetlXHelper/issues"
    },
    "contributes": {
        "languages": [{
            "id": "setlx",
            "aliases": ["SetlX", "setlx"],
            "extensions": [".stlx",".setlx"],
            "configuration": "./setlx.configuration.json"
        }],
        "grammars": [{
            "language": "setlx",
            "scopeName": "source.setlx",
            "path": "./syntaxes/setlx.tmLanguage"
        }],
        "snippets": [
            {
                "language": "setlx",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}